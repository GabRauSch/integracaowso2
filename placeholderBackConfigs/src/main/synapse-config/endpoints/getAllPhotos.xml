<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="getAllPhotos" xmlns="http://ws.apache.org/ns/synapse">
    <http method="get" uri-template="http://jsonplaceholder.typicode.com/photos">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
